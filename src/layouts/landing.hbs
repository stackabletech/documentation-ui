<!DOCTYPE html>
<html lang="en">
  <head>
{{> head defaultPageTitle='Untitled'}}
  </head>
  <body class="article{{#with (or page.attributes.role page.role)}} {{{this}}}{{/with}}">
{{> header}}
    <div class="body">
        <main class="article">
        <div class="content">
            <article class="doc landing">
            {{#with page.title}}
            <h1 class="page">{{{this}}}</h1>
            {{/with}}
            {{{page.contents}}}
            {{> pagination}}
            </article>
        </div>
        </main>
    </div>
{{> footer}}
  </body>
</html>
